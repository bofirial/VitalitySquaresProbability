{
  "compilerOptions": {
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "../../wwwroot/js",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [ "es2015", "dom" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot/js"
  ]
}