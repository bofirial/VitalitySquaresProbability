{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "../../wwwroot/js",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "module": "commonjs"
  },
  "exclude": [
    "node_modules",
    "wwwroot/js"
  ]
}
